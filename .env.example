# Clerk Authentication Keys
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=your_clerk_publishable_key
CLERK_SECRET_KEY=your_clerk_secret_key

# Clerk API URLs
NEXT_PUBLIC_CLERK_FRONTEND_API=your_clerk_frontend_api_url
CLERK_API_URL=your_clerk_api_url
CLERK_JWKS_URL=your_clerk_jwks_url
CLERK_WEBHOOK_SECRET=your_clerk_webhook_secret

# Application URLs
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_API_URL=http://localhost:8000

# JWT Configuration
JWT_SECRET=your_secure_jwt_secret_here

# API Keys for URL Scanner
# PhishTank API Key
PHISHTANK_API_KEY=your_phishtank_api_key

# AlienVault OTX API Key
OTX_API_KEY=your_otx_api_key_here

# VirusTotal API Key
VIRUSTOTAL_API_KEY=your_virustotal_api_key_here

# Gemini API Key
GEMINI_API_KEY=your_gemini_api_key_here

# PostgreSQL Connection (Supabase Transaction Pooler)
POSTGRES_HOST=your_postgres_host
POSTGRES_PORT=your_postgres_port
POSTGRES_DATABASE=your_database_name
POSTGRES_USER=your_database_user
POSTGRES_PASSWORD=your_database_password

# Database URL for Prisma
DATABASE_URL=postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@${POSTGRES_HOST}:${POSTGRES_PORT}/${POSTGRES_DATABASE}

# Authentication Settings
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id
NEXT_PUBLIC_GOOGLE_REDIRECT_URI=your_google_redirect_uri

# Node Environment
NODE_ENV=development
PORT=3000 